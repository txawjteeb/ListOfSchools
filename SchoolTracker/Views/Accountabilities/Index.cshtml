            @Html.DisplayNameFor(model => model.IdleSignOrNot)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OCStaffAssigned)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfContact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StaffLastContacted)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NoteID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReadActiveOrNot)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdleSignOrNot)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
